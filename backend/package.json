{
  "name": "telegram-signals-marketplace-backend",
  "version": "1.0.0",
  "description": "Backend API for Telegram Signals Marketplace",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "worker:bot-queue": "ts-node src/workers/botQueueWorker.ts",
    "worker:bot-queue:dev": "ts-node-dev --respawn --transpile-only src/workers/botQueueWorker.ts",
    "worker:scheduler": "ts-node src/workers/schedulerWorker.ts",
    "worker:scheduler:dev": "ts-node-dev --respawn --transpile-only src/workers/schedulerWorker.ts",
    "worker:notifications": "ts-node src/workers/notificationWorker.ts",
    "worker:notifications:dev": "ts-node-dev --respawn --transpile-only src/workers/notificationWorker.ts",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:make": "knex migrate:make",
    "reindex:listings": "ts-node src/scripts/reindexListings.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "telegram",
    "marketplace",
    "cryptocurrency",
    "signals"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.478.0",
    "@aws-sdk/client-sqs": "^3.478.0",
    "@elastic/elasticsearch": "^9.3.0",
    "@simplewebauthn/server": "^9.0.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "redis": "^4.6.11",
    "telegraf": "^4.15.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
